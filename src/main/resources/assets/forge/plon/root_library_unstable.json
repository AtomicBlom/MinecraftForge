{
  "#model_clip_aux": ["#lambda",
    ["#name"],
    ["#lambda",
      ["#time"],
      ["#list", "#name", "#time", {}]
    ]
  ],

  "#trigger_positive": ["#lambda",
    ["#name", "#clip", "#parameter", "#event"],
    ["#bind",
      {
        "#name": ["#car", "#clip"],
        "#time": ["#car", ["#cdr", "#clip"]],
        "#triggers": ["#car", ["#cdr", ["#cdr", "#clip"]]]
      },
      ["#list", "#name", "#time", ["#conm", "#name", ["#list", "#parameter", "#event"], "#triggers"]]
    ]
  ],

  // definition for computing current clip
  "#asm_run": ["#lambda", ["#asm", "#state"], [[["#asm", " states"], "#state"]]],

  // definition for getting the raw stuff
  "#asm_def": ["#lambda", ["#asm"], {
    " states": ["#keys", ["#asm", " states"]],
    " transitions": ["#asm", " transitions"],
    " start_state": ["#asm", " start_state"]
  }],

  "#range": ["#lambda", ["#f", "#g"], ["-", "#g", "#f"]],

  // A -> (() -> A)
  "#delay": ["#macro", ["#A", ["#A"]], ["#v"], ["#list", ["#quote", "#lambda"], [], "#v"]],

  // map -> (() -> map)
  "#delay_values": ["#macro", [" map", [" map"]], ["#m"], ["#list", ["#quote", "#mfold"], ["#quote", ["#lambda", ["#r", "#k", "#v"], ["#conm", "#k", ["#delay", "#v"], "#r"]]], {}, "#m"]],

  "#keys":   ["#lambda", ["#m"], ["#mfold", ["#lambda", ["#r", "#k", "#v"], ["#cons", "#k", "#r"]], [], "#m"]],
  "#values": ["#lambda", ["#m"], ["#mfold", ["#lambda", ["#r", "#k", "#v"], ["#cons", "#v", "#r"]], [], "#m"]]
}